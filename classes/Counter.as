package {	import flash.display.MovieClip;	public class Counter extends MovieClip {		public var currentValue:Number;		public var currentText:String;				public function Counter() {			reset();		}				public function makeValue (amountToMake:Number):void {			currentValue = Math.round(amountToMake);			updateDisplay();		}				public function makeText(stringToMake:String):void {			currentText = stringToMake;			updateDisplay();		}				public function reset():void {			currentValue = 0;			currentText = "null";			updateDisplay();		}				public function updateDisplay():void {					}	}}